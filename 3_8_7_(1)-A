#include <iostream>
#include <math.h>
using namespace std;
void increment(int& var, int expr = 1) {

	var += expr;

}

void increment(float& var, float expr = 1) {

	var += expr;

}


int main()
{
	int intvar = 0;
	float floatvar = 1.5;

	for (int i = 0; i < 10; i++) {
		if (i % 2) {
			increment(intvar);
			increment(floatvar, sqrt(intvar));
		}
		else {
			increment(intvar, i);
			increment(floatvar);
		}
	}

	cout << intvar * floatvar << endl;

	return 0;

}
