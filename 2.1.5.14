#include <stdio.h>

int main()
{
    int seven = 0;
    float pi = 0.0;
    printf("Enter 7: ");
    scanf("%d", &seven);
    printf("Enter pi(3.14): ");
    scanf("%f", &pi);
    printf("How many days in the week: %d\n", seven);
    printf("The value of Pi to two points: %.2f\n", pi);
    printf("There are %d days in the week\n", seven);
    printf("Pi value is %f\n", pi);
	/* your code */
	return 0;
}
